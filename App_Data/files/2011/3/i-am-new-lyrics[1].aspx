                function UpdateListenRC() {

                    if (songInfo != null) {
                        if (songInfo.length > 0) {
                            $("#listenOnlineRC a.songRCLink").html(ShortenString(songInfo[0].Title, 17));
                            $("#listenOnlineRC a.songRCLink").attr('href', songInfo[0].Lyrics);
                            
                            $("#listenOnlineRC a.artistRCLink").html(ShortenString(songInfo[0].Artist, 25));
                            $("#listenOnlineRC a.artistRCLink").attr('href', songInfo[0].ArtistLink);
                        }
                    }

                    if (showInfo != null) {
                        if (showInfo.length > 0) {

                            if (showInfo[0].Title != undefined && showInfo[0].Title.toLowerCase() != 'n/a') {
                                $("#listenOnlineRC .OnAir").show();
                                $("#listenOnlineRC a.showRCLink").html(showInfo[0].Title);
                                $("#listenOnlineRC a.showRCLink").attr('href', showInfo[0].Link);
                            }
                            else
                                $("#listenOnlineRC .OnAir").hide();                                         
                        }
                    }
                    
                    setTimeout("UpdateListenRC()", 30000);                                 
                }                         
            
            $(document).ready(function() {
            setTimeout("UpdateListenRC()", 30000);
